<Window x:Class="Unicorn.Toolbox.Visualization.WindowVisualization"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Unicorn.Toolbox.Visualization"
        mc:Ignorable="d"
        Title="Visualizator" Height="750" Width="1100" ResizeMode="NoResize">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <Canvas x:Name="canvasVisualization" Background="#FF1E1E1E" Width="1094"/>
        </ScrollViewer>
    </Grid>
</Window>
