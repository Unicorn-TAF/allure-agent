<?xml version="1.0"?>
<package >
  <metadata>
    <id>Unicorn.AllureAgent</id>
    <version>$version$</version>
    <title>Unicorn.AllureAgent</title>
    <authors>Vitaliy Dobriyan</authors>
    <license type="expression">Apache-2.0</license>
    <copyright>Copyright (c) Vitaliy Dobriyan 2024</copyright>
    <projectUrl>https://unicorn-taf.github.io</projectUrl>
    <repository type="git" url="https://github.com/Unicorn-TAF/allure-agent"/>
    <icon>images\icon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Unicorn.AllureAgent is Unicorn test automation framework extension which allows to report your tests execution into Allure report.
      This package includes ready for use implementation of Allure extension which is referenced by your tests.
    </description>
    <releaseNotes>Test fixtures reporting within test container, TMS and issue links</releaseNotes>
    <tags>unicorn-taf automation allure reporting</tags>
    <dependencies>
      <group targetFramework="net452">
        <dependency id="Allure.Commons" version="3.5.0.4"/>
        <dependency id="Unicorn.Taf.Core" version="3.3.0"/>
      </group>
      <group targetFramework="net5.0">
        <dependency id="Allure.Commons" version="3.5.0.4"/>
        <dependency id="Unicorn.Taf.Core" version="3.3.0"/>
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Allure.Commons" version="3.5.0.4"/>
        <dependency id="Unicorn.Taf.Core" version="3.3.0"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\icon.png" target="images\" />
    <file src="bin\Release\net452\unicorn.allureagent.dll" target="lib\net452" />
    <file src="bin\Release\net452\unicorn.allureagent.xml" target="lib\net452" />
    <file src="bin\Release\netstandard2.0\unicorn.allureagent.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\unicorn.allureagent.xml" target="lib\netstandard2.0" />
    <file src="bin\Release\net5.0\unicorn.allureagent.dll" target="lib\net5.0" />
    <file src="bin\Release\net5.0\unicorn.allureagent.xml" target="lib\net5.0" />
  </files>
</package>